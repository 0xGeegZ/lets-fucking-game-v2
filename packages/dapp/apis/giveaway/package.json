{
  "name": "giveaway-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler publish",
    "test": "concurrently --success first --kill-others \"wrangler dev\" \"wait-on -d 5000 -t 30000 http-get://127.0.0.1:8787 && node --experimental-vm-modules node_modules/.bin/jest --coverage\"",
    "lint": "eslint --fix . && echo 'Lint complete.'"
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js",
      "src/*.js"
    ]
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "itty-router": "^2.6.1",
    "itty-router-extras": "^0.4.2",
    "needle": "3.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.10.0",
    "@types/itty-router-extras": "^0.4.0",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yaml": "0.5.0",
    "jest": "29.3.1",
    "node-fetch": "3.3.0",
    "prettier": "2.8.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tsconfig": "*",
    "typescript": "^4.6.4",
    "wait-on": "6.0.1",
    "wrangler": "2.6.0"
  }
}
